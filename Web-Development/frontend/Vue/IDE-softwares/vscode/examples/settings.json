{
    /*
        VSCODE SPECIFIC SETTINGS FOR PROJECT: <CLIENT_NAME> <PRODUCT TYPE OR NAME> <YEAR RELEASE>
    */
    // PROJECT SETTINGS:
    //...
    // BACKEND SETTINGS:
    //...
    // VUE SETTINGS:
    "[vue]": {
        //"editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.tabSize": 2,
        "editor.indentSize": "tabSize",
        "editor.insertSpaces": true,
    },
    // FILE ASSOCIATIONS:
    "files.associations": {
        "*.vue": "vue",
    },
    // FILE EXCLUSIONS:
    "files.exclude": {
        "**/node_modules": false,
        "**/dist": false,
    },
    // SEARCH EXCLUSIONS:
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
    },
}